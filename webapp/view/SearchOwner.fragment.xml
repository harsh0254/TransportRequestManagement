<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="Transport Requests" contentWidth="30%" contentHeight="60%">
		<List headerText="Owner"  items="{ path: 'localModel3>/' }" mode="SingleSelectMaster" selectionChange="onListOwnerPressed">
			<StandardListItem title="{localModel3>Owner}" description="{localModel3>Name}"/>
		</List>
		<endButton>
			<Button text="Close" press="pressOnCloseDialog"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>